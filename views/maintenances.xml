<odoo>
    <data>

        <record id="view_maintenances_tree" model="ir.ui.view">
            <field name="name">view_maintenances_tree</field>
            <field name="model">management.maintenances</field>
            <field name="type">tree</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <tree string="maintenances">
                    <field name="date"/>
                    <field name="tiket"/>
                    <field name="tujuan"/>
                    <field name="status"/>
                </tree>
            </field>
        </record>

        <record id="view_maintenances_form" model="ir.ui.view">
            <field name="name">view_maintenances_form</field>
            <field name="model">management.maintenances</field>
            <field name="type">form</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <form string="maintenances Form">
                    <sheet>
                        <group>
                            <field name="date"/>
                            <field name="tiket"/>
                            <field name="tujuan"/>
                            <field name="status"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

    </data>
</odoo>